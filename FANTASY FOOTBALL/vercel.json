{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "upload-why-not/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!api/|assets/|favicon.ico|vite.svg).*)",
            "destination": "/index.html"
        }
    ]
}